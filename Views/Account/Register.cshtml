<h1>Skapa konto</h1>
<form asp-action="Register" method="post">
    <input type="text" name="username" placeholder="Välj namn" required /><br />
    <input type="password" name="password" placeholder="Välj lösenord" required /><br />
    <button type="submit">Registrera</button>
</form>
<p style="color:red">@ViewBag.Error</p>
<a href="/Account/Login">Har du redan konto? Logga in</a>